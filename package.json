{
  "name": "ngx-scroll-tracker",
  "version": "0.0.1",
  "description": "Angular X Scroll Tracker",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-scroll-tracker.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-scroll-tracker.umd.min.js",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicky-lenaers/ngx-scroll-tracker.git"
  },
  "keywords": [
    "ngx",
    "ngx-scroll-tracker",
    "angular",
    "angular2",
    "angular 2",
    "angular4",
    "angular 4"
  ],
  "author": "Nicky Lenaers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicky-lenaers/ngx-scroll-tracker/issues"
  },
  "homepage": "https://github.com/nicky-lenaers/ngx-scroll-tracker#readme",
  "engines": {
    "node": ">=7.10.0",
    "npm": ">=4.2.0"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^4.2.2",
    "@angular/core": "^4.2.2",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/compiler": "^4.2.2",
    "@angular/compiler-cli": "^4.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.16"
  }
}
